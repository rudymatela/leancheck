map length (tiers :: [[ Bool->Bool ]])  =  [4]

length (list :: [ Bool->Bool ])  =  4

allUnique (list :: [ Bool->Bool ])  =  True

ratioRepetitions (list :: [ Bool->Bool ])  =  0 % 1

tiers :: [Bool->Bool]  =
  [ [ \x -> case x of
              False -> False
              True -> False
    , \x -> case x of
              False -> True
              True -> False
    , \x -> case x of
              False -> False
              True -> True
    , \x -> case x of
              False -> True
              True -> True
    ]
  ]
