tiers :: [Nat->()]  =
  [ [ \x -> case x of
              0 -> ()
              1 -> ()
              2 -> ()
              3 -> ()
              4 -> ()
              5 -> ()
              6 -> ()
              7 -> ()
              ...
    ]
  , []
  , []
  , []
  , []
  , []
  , ...
  ]

map length (tiers :: [[ Nat->() ]])  =  [1,0,0,0,0,0,...]

length (list :: [ Nat->() ])  =  1
